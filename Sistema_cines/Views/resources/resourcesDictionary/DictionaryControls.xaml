<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:materialDesing="http://materialdesigninxaml.net/winfx/xaml/themes"
                    xmlns:local="clr-namespace:Views.resources.resourcesDictionary">

    <Style x:Key="ButtonG7" TargetType="Button" >
        <Setter Property="Style" Value="{StaticResource MaterialDesignRaisedDarkButton}"/>
    </Style>
    
    
    <Style x:Key="TextBoxABM" TargetType="TextBox">
        <Setter Property="Margin" Value="91,0,32,17"/>
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="Height" Value="34"/>
        <Setter Property="Foreground" Value="White" />
        <Setter Property="Background"  Value="Transparent"/>
    </Style>

    
</ResourceDictionary>